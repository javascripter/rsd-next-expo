{
  "dependencies": {
    "@rsd-next-expo/app": "workspace:*",
    "expo": "^50.0.14",
    "expo-build-properties": "~0.11.1",
    "expo-constants": "~15.4.5",
    "expo-image": "~1.10.6",
    "expo-linear-gradient": "~12.7.2",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.8",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.10",
    "source-map-explorer": "^2.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@types/react": "^18.2.67",
    "@types/react-native": "~0.72.8",
    "typescript": "~5.3.0",
    "jest-expo": "~50.0.4",
    "@testing-library/react-native": "^12.4.4"
  },
  "scripts": {
    "dev": "expo start --dev-client",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build": "expo export",
    "build:web": "expo export --platform web",
    "build:ios": "npx expo export --platform ios --no-bytecode",
    "build:android": "npx expo export --platform android",
    "analyze:web": "npx expo export --source-maps --platform web && source-map-explorer 'dist/_expo/static/js/web/*.js' 'dist/_expo/static/js/web/*.js.map'",
    "analyze:ios": "npx expo export --source-maps --platform ios --no-bytecode && source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'",
    "analyze:android": "npx expo export --source-maps --platform ios --no-bytecode && source-map-explorer 'dist/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'"
  },
  "main": "index.js",
  "version": "0.0.0",
  "private": true,
  "name": "@rsd-next-expo/expo"
}
